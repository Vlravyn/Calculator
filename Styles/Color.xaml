<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="WhiteBlue">#F7FBFC</Color>
    <Color x:Key="VeryLightBlue">#D6E6F2</Color>
    <Color x:Key="Snow">#B9D7EA</Color>
    <Color x:Key="OldBlue">#769FCD</Color>
    <Color x:Key="SuccessGreen">#53A653</Color>
    <Color x:Key="SuccessGreenLight">#4BB543</Color>
    <Color x:Key="SuccessGreenDark">#2E5C2E</Color>

    <SolidColorBrush x:Key="WhiteBlueBrush" Color="{StaticResource WhiteBlue}" />
    <SolidColorBrush x:Key="VeryLightBlueBrush" Color="{StaticResource VeryLightBlue}" />
    <SolidColorBrush x:Key="SnowBrush" Color="{StaticResource Snow}" />
    <SolidColorBrush x:Key="OldBlueBrush" Color="{StaticResource OldBlue}" />
    <SolidColorBrush x:Key="SuccessGreenBrush" Color="{StaticResource SuccessGreen}" />
    <SolidColorBrush x:Key="SuccessGreenLightBrush" Color="{StaticResource SuccessGreenLight}" />
    <SolidColorBrush x:Key="SuccessGreenDarkBrush" Color="{StaticResource SuccessGreenDark}" />
</ResourceDictionary>